generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model Asset {
  id           Int      @id @default(autoincrement())
  serialNumber String   @unique
  name         String
  purchaseDate String?  
  status       String   @default("Em Estoque")
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt 


  @@map("assets")
}
